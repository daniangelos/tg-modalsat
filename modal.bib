@article{Ohlbach93c, 
author = "H. J. Ohlbach",
title = "Translation Methods for Non-Classical Logics -- An Overview",
journal = "Journal of the IGPL",
year =  1993, 
volume = 1,
number = 1,
pages = "69--90",
location = "HJO",
abstract = {This paper gives an overview on translation methods we have developed
for nonclassical logics, in particular for modal logics.\ Optimized
`functional' and semi-functional translation into predicate logic is
described. Using normal modal logic as an intermediate logic, other
logics can be translated into predicate logic as well. As an example,
the translation of modal logic of graded modalities is sketched. In the
second part of the paper it is shown how to translate Hilbert axioms
into properties of the semantic structure and vice versa, i.e. we 
can automate important parts of correspondence theory. 
The exact formalisms and the
soundness and completeness proofs can be found in the original
papers.},
keywords = {Modal Logic, Functional Translation},
source = "ftp://pub/guide/staff/ohlbach/publications/articles/igpl.dvi.gz"}}
}

